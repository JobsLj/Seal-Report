@using Seal.Model
@using Seal.Helpers
@using SealWebServer.Controllers
@{
    Repository repository = Model as Repository;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@repository.Configuration.WebProductName</title>
    <meta charset="utf-8" />
    <link href="~/Images/favicon.ico" rel="shortcut icon" type="image/x-icon" />

    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/font-awesome.min.css" />
    <link rel='stylesheet' href='~/Content/jstree-style.min.css' />
    <link rel="stylesheet" href="~/Content/bootstrap-select.min.css" />
    <link rel='stylesheet' href='~/Content/bootstrap-datetimepicker.min.css' />
    <link rel="stylesheet" href="~/Content/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/responsive.bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/SealWeb.css" />
</head>
<body>
    <nav class="navbar navbar-default navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">@repository.TranslateWeb("Toggle navigation")</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a id="brand-id" class="navbar-brand" href="#">@repository.Configuration.WebProductName</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse" aria-expanded="false">
                <ul class="nav navbar-nav pull-xs-right navbar-right">
                    <li class="nav-item"><a id="profile-nav-item">@repository.TranslateWeb("Profile")</a></li>
                    <li class="nav-item"><a id="disconnect-nav-item">@repository.TranslateWeb("Disconnect")</a></li>
                </ul>
                <form class="navbar-form navbar-right">
                    <div class="input-group">
                        <input id="search-pattern" type="text" class="form-control input-sm col-sm-1" placeholder="@repository.TranslateWeb("Search")" />
                        <span class="input-group-btn">
                            <button id="search-nav-item" type="button" class="btn btn-sm btn-success"><span class="glyphicon glyphicon-search"></span></button>
                        </span>
                    </div>
                </form>
                <ul class="nav navbar-nav pull-xs-right navbar-right">
                    <li class="nav-item hidden-xs"><a id="folders-nav-item" title="Manage folders">@repository.TranslateWeb("Folders")</a></li>
                    <li class="nav-item"><a id="refresh-nav-item">@repository.TranslateWeb("Refresh")</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
    </nav>

    <div id="main-container" class="container-fluid" style="padding-top:62px; display:none;">
        <div class="row">
            <div id="folder-view" class="col-sm-4" style="padding-right:0px">
                <div class="jstree jstree-default" id="folder-tree" role="tree" style="overflow:auto;"></div>
            </div>

            <div id="file-view" class="col-sm-8" style="padding-right:0px">
                <!-- Report Menus -->
                <div id="file-menu" class="lighticon-menu hidden-xs" style="margin-bottom:10px">
                    <div id="report-edit-lightbutton" class="lightbutton report-edit"><span class="lightbutton-icon fa fa-file-o"></span><br /><span class="lightbutton-text">@repository.TranslateWeb("New")</span></div>
                    <div id="report-cut-lightbutton" class="lightbutton"><span class="lightbutton-icon fa fa-cut"></span><br /><span class="lightbutton-text">@repository.TranslateWeb("Cut")</span></div>
                    <div id="report-copy-lightbutton" class="lightbutton"><span class="lightbutton-icon fa fa-copy"></span><br /><span class="lightbutton-text">@repository.TranslateWeb("Copy")</span></div>
                    <div id="report-paste-lightbutton" class="lightbutton"><span class="lightbutton-icon fa fa-paste"></span><br /><span class="lightbutton-text">@repository.TranslateWeb("Paste")</span></div>
                    <div id="report-delete-lightbutton" class="lightbutton"><span class="lightbutton-icon fa fa-remove"></span><br /><span class="lightbutton-text">@repository.TranslateWeb("Delete")</span></div>
                    <div id="report-rename-lightbutton" class="lightbutton"><span class="lightbutton-icon fa fa-file-text"></span><br /><span class="lightbutton-text">@repository.TranslateWeb("Rename")</span></div>
                </div>

                <div id="file-table-view" style="overflow:auto;">
                    <table id="file-table" class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th style="width:22px;" class="hidden-xs"><input id="selectall-checkbox" type="checkbox" /></th>
                                <th>@repository.TranslateWeb("Report")</th>
                                <th id="action-tableheader">@repository.TranslateWeb("Actions")</th>
                                <th style="width: 170px; min-width:170px;" class="hidden-xs">@repository.TranslateWeb("Last modification")</th>
                            </tr>
                        </thead>
                        <tbody id="file-table-body"></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>


    <!--Wait Dialog-->
    <div class="modal fade in" id="wait-dialog" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body" style="text-align:center">
                    <i class="fa fa-spinner fa-spin fa-2x fa-fw"></i>
                    <h3 style="display:inline">@repository.TranslateWeb("Please wait")...</h3>
                </div>
            </div>
        </div>
    </div>

    <!-- Views and Output -->
    <div id="output-panel" class="panel panel-default" style="display:none; border-color:black;">
        <div class="panel-heading" style="height:20px;padding: 0px 10px;"><button type='button' id="output-panel-close" class='close'>&times;</button></div>
        <div id="output-panel-content">
            <table class="table table-striped">
                <thead>
                    <tr><th>@repository.TranslateWeb("Name")</th><th>@repository.TranslateWeb("Type")</th></tr>
                </thead>
                <tbody id="output-table-body"></tbody>
            </table>
        </div>
    </div>

    <!-- Editor -->
    <div id="edit-dialog" role="dialog" class="modal fade in" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog" style="width: 900px;">
            <div class="modal-content">
                <div class="modal-header" style="padding: 0px;">
                    <nav id="dlgnav" class="navbar navbar-default" style="display: block; padding-right: 5px; margin-bottom: 0px;">
                        <div class="container-fluid">
                            <ul class="nav pagination" style="margin: 10px;">
                                <li id="nav-dialog-elements" class="active"><a><span class="fa fa-cubes"></span> @repository.TranslateWeb("Elements")</a></li>
                                <li id="nav-dialog-options"><a href="#"><span class="fa fa-list-ul"></span> @repository.TranslateWeb("Options")</a></li>
                                <li id="nav-dialog-schedule"><a href="#"><span class="fa fa-clock-o"></span> @repository.TranslateWeb("Schedule")</a></li>
                            </ul>
                            <ul class="nav navbar-nav navbar-right" style="margin-right:0px">
                                <li><button id="nav-dialog-execute" class="btn btn-default" style="margin:10px 1px"><span class="fa fa-cog"></span> @repository.TranslateWeb("Execute")</button></li>
                                <!--   <li><button id="nav-dialog-render" class="btn btn-default" style="margin:10px 1px"><span class="fa fa-cogs"></span> Render</button></li> -->
                                <li><button id="nav-dialog-save" class="btn btn-default" style="margin:10px 1px"><span class="fa fa-save"></span> @repository.TranslateWeb("Save")</button></li>
                                <!--   <li class="dropdown">
                                       <a class="dropdown-toggle" data-toggle="dropdown" href="#" style="padding-left:0px;padding-right:15px;">
                                           <span class="caret"></span>
                                       </a>
                                       <ul class="dropdown-menu">
                                           <li><a href="#">Save as...</a></li>
                                       </ul>
                                   </li>-->
                                <li><button id="nav-dialog-close" class="btn btn-primary" style="margin:10px 1px" href="#"><span class="fa fa-close"></span> @repository.TranslateWeb("Close") </button></li>
                            </ul>
                        </div>
                    </nav>
                </div>
                <div class="modal-body" style="padding-top: 5px; padding-bottom: 5px;">
                    <div id="elements-row" class="row editor-panel">
                        <div style="padding:0px 2px;" class="col-md-4">
                            <div style="margin-left:4px;">
                                <select id="source-select" data-width="100%"></select>
                            </div>
                            <div class="jstree jstree-default drag-panel" style="padding:0px;margin:5px; height:420px; overflow:auto;" id="elements-tree" role="tree">
                            </div>
                            <hr style="margin:4px 0px;" />
                            <div style="text-align:center;">
                                <button id="grid-mode-btn" type="button" class="btn btn-default active"><i class="fa fa-table"></i> @repository.TranslateWeb("Grid mode")</button>
                                <button id="pivot-mode-btn" type="button" class="btn btn-default"><i class="fa fa-th-large"></i> @repository.TranslateWeb("Pivot mode")</button>
                            </div>
                        </div>
                        <div style="padding:0px 2px;" class="col-md-8">
                            <div class="container-fluid" style="padding: 0px 2px;">
                                <row>
                                    <div id="page-panel" class="panel panel-success col-md-6 drag-panel">
                                        <span class="drop-span"></span>
                                    </div>
                                    <div id="col-panel" class="panel panel-success col-md-6 drag-panel">
                                        <span class="drop-span"></span>
                                    </div>
                                </row>
                                <row>
                                    <div id="row-panel" class="panel panel-success col-md-6 drag-panel">
                                        <span class="drop-span"></span>
                                    </div>
                                    <div id="data-panel" class="panel panel-success col-md-6 drag-panel">
                                        <span class="drop-span"></span>
                                    </div>
                                </row>
                                <row>
                                    <div id="restriction-panel" class="panel panel-success col-md-12 drag-panel" style="padding:0px 0px;margin-bottom: 0px;">
                                        <span class="drop-span"></span>
                                    </div>
                                </row>
                            </div>
                        </div>
                    </div>
                    <div id="options-row" class="row editor-panel">
                        <div class="form-horizontal col-sm-6" role="form">
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="option-format">@repository.TranslateWeb("Format")</label>
                                <div class="col-sm-8">
                                    <select id="option-format" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="option-title">@repository.TranslateWeb("Report title")</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="option-title" placeholder="@repository.TranslateWeb("Report title if different from report name")" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="option-description">@repository.TranslateWeb("Description")</label>
                                <div class="col-sm-8">
                                    <textarea class="form-control" id="option-description" rows="3"></textarea>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="option-connection">@repository.TranslateWeb("Connection")</label>
                                <div class="col-sm-8">
                                    <select id="option-connection" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group option-chart">
                                <label class="control-label col-sm-4" for="option-chart-title">@repository.TranslateWeb("Chart title")</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="option-chart-title" placeholder="@repository.TranslateWeb("Optional title chart")" />
                                </div>
                            </div>
                            <div class="form-group option-chart">
                                <label class="control-label col-sm-4" for="option-chart-height">@repository.TranslateWeb("Chart height")</label>
                                <div class="col-sm-8">
                                    <input class="form-control numeric" id="option-chart-height" />
                                </div>
                            </div>
                            <div class="form-group option-chart">
                                <label class="control-label col-sm-4" for="option-chart-width">@repository.TranslateWeb("Chart width")</label>
                                <div class="col-sm-8">
                                    <input class="form-control numeric" id="option-chart-width" />
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 ">
                            <div class="form-horizontal panel panel-default" role="form">
                                <div class="panel-heading">
                                    <h1 class="panel-title">@repository.TranslateWeb("Display options")</h1>
                                </div>
                                <div id="option-body" class="panel-body">
                                </div>
                            </div>

                        </div>
                    </div>
                    <div id="schedule-row" class="row editor-panel">
                        <div class="form-horizontal col-sm-6" role="form">
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="output-name">@repository.TranslateWeb("Output")</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <input id="output-name" type="text" class="form-control" />
                                        <div class="input-group-btn">
                                            <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                                <span class="caret"></span>
                                            </button>
                                            <ul id="output-name-dropdown" class="dropdown-menu"></ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group output-execute">
                                <label class="control-label col-sm-4" for="output-format">@repository.TranslateWeb("Format")</label>
                                <div class="col-sm-8">
                                    <select id="output-format" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group output-folder">
                                <label class="control-label col-sm-4" for="output-folder">@repository.TranslateWeb("Folder")*</label>
                                <div class="col-sm-8">
                                    <select id="output-folder" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group output-folder">
                                <label class="control-label col-sm-4" for="output-file">@repository.TranslateWeb("File name")*</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <input id="output-file" type="text" class="form-control" placeholder="@repository.TranslateWeb("Enter the file name without extension")" />
                                        <div class="input-group-btn">
                                            <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                                <span class="caret"></span>
                                            </button>
                                            <ul id="output-file-dropdown" class="dropdown-menu"></ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-to">@repository.TranslateWeb("To")*</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="output-to" placeholder="@repository.TranslateWeb("Enter addresses separated by ;")" />
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-subject">@repository.TranslateWeb("Subject")</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="output-subject" placeholder="@repository.TranslateWeb("Default subject")" />
                                </div>
                            </div>
                            <div class="form-group output-email" style="margin-bottom: 5px;">
                                <label class="control-label col-sm-4" for="output-body">@repository.TranslateWeb("Body")</label>
                                <div class="col-sm-8">
                                    <textarea class="form-control" id="output-body" rows="2" placeholder="@repository.TranslateWeb("Default body")"></textarea>
                                </div>
                            </div>
                            <hr class="output-email" style="margin:4px 0px;" />
                            <div class="form-group output-email" id="output-from-panel">
                                <label class="control-label col-sm-4" for="output-from">@repository.TranslateWeb("From")</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="output-from" placeholder="@repository.TranslateWeb("Default sender address")" />
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-cc">@repository.TranslateWeb("CC")</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="output-cc" placeholder="@repository.TranslateWeb("Optional addresses separated by ;")" />
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-bcc">@repository.TranslateWeb("BCC")</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="output-bcc" placeholder="@repository.TranslateWeb("Optional addresses separated by ;")" />
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-html">@repository.TranslateWeb("Result as body")</label>
                                <div class="col-sm-8">
                                    <select id="output-html" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-zip">@repository.TranslateWeb("Zip attachments")</label>
                                <div class="col-sm-8">
                                    <select id="output-zip" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group output-email">
                                <label class="control-label col-sm-4" for="output-zippassword">@repository.TranslateWeb("Zip password")</label>
                                <div class="col-sm-8">
                                    <input type="password" class="form-control" id="output-zippassword" placeholder="@repository.TranslateWeb("Enter a password to protect the Zip file")" />
                                </div>
                            </div>
                            <hr class="output-execute" style="margin:4px 0px;" />
                            <div class="form-group output-execute">
                                <label class="control-label col-sm-4" for="output-publicedit">@repository.TranslateWeb("Edition")</label>
                                <div class="col-sm-3">
                                    <select id="output-publicedit" data-width="100%"></select>
                                </div>
                            </div>
                            <div class="form-group output-execute">
                                <label class="control-label col-sm-4" for="output-publicedit">@repository.TranslateWeb("Execution")</label>
                                <div class="col-sm-3">
                                    <select id="output-publicexec" data-width="100%"></select>
                                </div>
                                <div class="col-sm-5" style="text-align:right">
                                    <button id="nav-dialog-execute-schedule" class="btn btn-success" style="text-align:right"><span class="fa fa-bolt"></span> @repository.TranslateWeb("Execute now")</button>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 ">
                            <div class="form-horizontal panel panel-default" role="form">
                                <div class="panel-heading">
                                    <i class="fa fa-clock-o"></i><h1 class="panel-title" style="display:inline"> @repository.TranslateWeb("Execution")</h1>
                                </div>
                                <div id="schedule-exec" class="panel-body" style="height:340px">
                                    <div class="form-group">
                                        <label class="control-label col-sm-5" for="schedule-instance">@repository.TranslateWeb("Name")</label>
                                        <div class="col-sm-7">
                                            <div class="input-group">
                                                <input id="schedule-instance" type="text" class="form-control" />
                                                <div class="input-group-btn">
                                                    <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                                        <span class="caret"></span>
                                                    </button>
                                                    <ul id="schedule-instance-dropdown" class="dropdown-menu"></ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-start">@repository.TranslateWeb("Start date")</label>
                                        <div class="col-sm-7">
                                            <div class="input-group date">
                                                <input id="schedule-start" type='text' class="form-control" />
                                                <span class="input-group-addon">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-end">@repository.TranslateWeb("End date")</label>
                                        <div class="col-sm-7">
                                            <div class="input-group date">
                                                <input id="schedule-end" type='text' class="form-control" />
                                                <span class="input-group-addon">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-enabled">@repository.TranslateWeb("Is enabled")</label>
                                        <div class="col-sm-7">
                                            <select class="form-control" id="schedule-enabled"></select>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-type">@repository.TranslateWeb("Type")</label>
                                        <div class="col-sm-7">
                                            <select id="schedule-type" data-width="100%"></select>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-end">@repository.TranslateWeb("Recur every")</label>
                                        <div class="col-sm-4">
                                            <input class="form-control numeric" id="schedule-interval" />
                                        </div>
                                        <label class="control-label col-sm-3" id="schedule-interval-label" style="text-align:left;padding-left: 0px;"></label>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-weekdays">@repository.TranslateWeb("Days of the week")</label>
                                        <div class="col-sm-7">
                                            <select id="schedule-weekdays" multiple="multiple"></select>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-months">@repository.TranslateWeb("Months")</label>
                                        <div class="col-sm-7">
                                            <select id="schedule-months" multiple="multiple"></select>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-days">@repository.TranslateWeb("Days")</label>
                                        <div class="col-sm-7">
                                            <select id="schedule-days" multiple="multiple"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel-footer" style="height:110px">
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-repeat1">@repository.TranslateWeb("Repeat every")</label>
                                        <div class="col-sm-7">
                                            <div class="input-group">
                                                <input id="schedule-repeat1" type="text" class="form-control" />
                                                <div class="input-group-btn">
                                                    <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                                        <span class="caret"></span>
                                                    </button>
                                                    <ul id="schedule-repeat1-dropdown" class="dropdown-menu"></ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group schedule">
                                        <label class="control-label col-sm-5" for="schedule-repeat2">@repository.TranslateWeb("for a duration of")</label>
                                        <div class="col-sm-7">
                                            <div class="input-group">
                                                <input id="schedule-repeat2" type="text" class="form-control" />
                                                <div class="input-group-btn">
                                                    <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                                        <span class="caret"></span>
                                                    </button>
                                                    <ul id="schedule-repeat2-dropdown" class="dropdown-menu"></ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div style="padding:2px;" class="modal-footer">
                    <row>
                        <div class="col-md-8 text-left">
                            <ol class="breadcrumb" id="breadcrumb" style="display: inline;margin: 0px 5px;padding: 0px"></ol>
